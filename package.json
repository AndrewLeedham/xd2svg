{
  "name": "xd2svg",
  "version": "0.5.0",
  "description": "Utility for converting Adobe XD files (*.xd) to SVG",
  "keywords": [
    "svg",
    "xd",
    "adobe xd",
    "adobe experience design",
    "converter",
    "vector",
    "graphics"
  ],
  "homepage": "https://github.com/L2jLiga/xd2svg#xd2svg",
  "bugs": {
    "url": "https://github.com/L2jLiga/xd2svg/issues"
  },
  "license": "MIT",
  "author": "L2jLiga",
  "files": [
    "bin",
    "dist"
  ],
  "main": "dist/xd2svg-cli.js",
  "bin": {
    "xd2svg-cli": "bin/xd2svg-cli"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/L2jLiga/xd2svg.git"
  },
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "clean": "rimraf dist",
    "lint": "tslint -c tslint.json -p tsconfig.json 'src/**/*.ts'",
    "prepublish": "npm run lint && npm run clean && npm run build"
  },
  "dependencies": {
    "extract-zip": "1.6.7",
    "jsdom": "11.11.0",
    "svgo": "1.0.5",
    "tmp": "0.0.33"
  },
  "devDependencies": {
    "@types/extract-zip": "1.6.2",
    "@types/jsdom": "11.0.6",
    "@types/node": "8.9.5",
    "@types/svgo": "1.0.1",
    "@types/tmp": "0.0.33",
    "copy-webpack-plugin": "^4.5.1",
    "rimraf": "2.6.2",
    "ts-loader": "4.4.1",
    "ts-node": "6.1.2",
    "tslint": "5.10.0",
    "typescript": "2.9.2",
    "webpack": "4.12.0",
    "webpack-cli": "3.0.8"
  },
  "engines": {
    "node": ">= 8.6.0"
  },
  "preferGlobal": true
}
