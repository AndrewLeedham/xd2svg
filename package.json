{
  "name": "xd2svg",
  "version": "0.3.1",
  "description": "Utility for converting Adobe XD files (*.xd) to SVG",
  "keywords": [
    "svg",
    "xd",
    "adobe xd",
    "adobe experience design",
    "converter",
    "vector",
    "graphics"
  ],
  "homepage": "https://github.com/L2jLiga/xd2svg#xd2svg",
  "bugs": {
    "url": "https://github.com/L2jLiga/xd2svg/issues"
  },
  "license": "MIT",
  "author": "L2jLiga",
  "files": [
    "bin",
    "dist"
  ],
  "main": "dist/xd2svg-cli.js",
  "bin": {
    "xd2svg-cli": "bin/xd2svg-cli"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/L2jLiga/xd2svg.git"
  },
  "scripts": {
    "build": "npm run build:cli && npm run build:ui",
    "build:cli": "webpack --config webpack.config.js",
    "build:ui": "echo \"UI isn't ready yet\"",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "extract-zip": "1.6.7",
    "jsdom": "11.11.0",
    "svgo": "1.0.5",
    "tmp": "0.0.33"
  },
  "devDependencies": {
    "@types/extract-zip": "1.6.2",
    "@types/jsdom": "11.0.4",
    "@types/node": "8.9.5",
    "@types/svgo": "1.0.1",
    "@types/tmp": "0.0.33",
    "ts-loader": "4.3.0",
    "ts-node": "6.0.5",
    "tslint": "5.10.0",
    "typescript": "2.8.3",
    "webpack": "4.9.1",
    "webpack-cli": "2.1.4"
  },
  "engines": {
    "node": ">= 8.9.4",
    "npm": ">= 5.3.0",
    "yarn": ">= 1.2.0"
  },
  "preferGlobal": true
}
